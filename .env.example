# ===========================================
# T3 Chat — Environment Variables
# ===========================================
# Copy this file to .env.local and fill in your API keys.
# On Vercel, set these in: Settings → Environment Variables
# API keys are NEVER exposed to the client — they are read server-side only.

# PostgreSQL Database URL
DATABASE_URL="postgresql://user:password@localhost:5432/t3chat?schema=public"

# OpenAI API Key (for GPT-4o, GPT-4o-mini)
OPENAI_API_KEY=

# Anthropic API Key (for Claude 3.5 Sonnet, Claude 3 Haiku)
ANTHROPIC_API_KEY=

# Google Generative AI API Key (for Gemini Pro, Gemini Flash)
GOOGLE_GENERATIVE_AI_API_KEY=
